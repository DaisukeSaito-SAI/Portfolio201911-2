<h1>登録画面</h1>

    <%= form_for(@user, url: "/users/create",method: :post) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>
</br>
      <%= f.label :email %>
      <%= f.email_field :email %>
</br>

      <%= f.label :password %>
      <%= f.password_field :password %>
</br>

      <%= f.label :password_confirmation, "Confirmation" %>
      <%= f.password_field :password_confirmation %>
</br>
      
      <%= f.submit "登録" %>
    <% end %>