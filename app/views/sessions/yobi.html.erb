      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :email %>
      <%= f.text_field :email %>
      
      
        def create
    @user = User.find_by(
      name: params[:session][:name],
      email: params[:session][:email].downcase)
  if @user
    log_in @user
    redirect_to "/"
  else
    render 'new'
  end
  end